<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.reclipse.structure.specification.metamodel">
      <metamodel
            description="This model  is referred to as the source code decorator model and it combines the SAMM and the GAST model into one."
            id="eu.qimpress.sourcecodedecorator"
            labeler="org.reclipse.models.SourceCodeDecoratorElementLabeler"
            name="Combined SAMM and GAST model"
            version="1.0">
